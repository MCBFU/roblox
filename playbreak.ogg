local function replaceSingleImage(obj,item)
    if item.newId == "000" then return end
    local targetOld = "rbxassetid://" .. tostring(item.oldId)
    local targetNew = "rbxassetid://" .. tostring(item.newId)
    if obj:IsA("Decal") or obj:IsA("Texture") then
        if obj.Texture == targetOld then obj.Texture = targetNew end
    else
        if obj.Image == targetOld then obj.Image = targetNew end
    end
end

local function replaceSingleSound(obj,item)
    local targetOld = "rbxassetid://" .. tostring(item.old)
    if obj.SoundId == targetOld then
        obj.SoundId = item.new == "" and "" or "rbxassetid://" .. tostring(item.new)
    end
end

local function replaceAll()
    task.spawn(function()
        local imageObjects = {}
        for _,obj in pairs(game:GetDescendants()) do
            if obj:IsA("ImageLabel") or obj:IsA("ImageButton") or obj:IsA("Decal") or obj:IsA("Texture") then
                table.insert(imageObjects,obj)
            end
        end
        for _,item in ipairs(replacementLists.Images) do
            if item.oldId and item.newId and item.oldId ~= "" and item.newId ~= "" then
                for _,obj in ipairs(imageObjects) do
                    replaceSingleImage(obj,item)
                end
            end
        end
    end)

    task.spawn(function()
        local soundObjects = {}
        for _,obj in pairs(game:GetDescendants()) do
            if obj:IsA("Sound") then
                table.insert(soundObjects,obj)
            end
        end
        for _,item in ipairs(replacementLists.Sounds) do
            if item.old and item.old ~= "" then
                for _,obj in ipairs(soundObjects) do
                    replaceSingleSound(obj,item)
                end
            end
        end
    end)

    game.DescendantAdded:Connect(function(obj)
        task.spawn(function()
            if obj:IsA("ImageLabel") or obj:IsA("ImageButton") or obj:IsA("Decal") or obj:IsA("Texture") then
                for _,item in ipairs(replacementLists.Images) do
                    if item.oldId and item.newId and item.oldId ~= "" and item.newId ~= "" then
                        replaceSingleImage(obj,item)
                    end
                end
            elseif obj:IsA("Sound") then
                for _,item in ipairs(replacementLists.Sounds) do
                    if item.old and item.old ~= "" then
                        replaceSingleSound(obj,item)
                    end
                end
            end
        end)
    end)
end

