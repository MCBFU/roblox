local function replaceAll()
    -- å›¾ç‰‡æ›¿æ¢
    for _, item in ipairs(replacementLists.Images) do
        if item.oldId ~= "" and item.newId ~= "" then
            for _, obj in pairs(game:GetDescendants()) do
                if obj:IsA("ImageLabel") or obj:IsA("ImageButton") then
                    local image = tostring(obj.Image)
                    if image == item.oldId or 
                       image == "rbxassetid://" .. item.oldId or
                       image:find(item.oldId) then
                        obj.Image = "rbxassetid://" .. item.newId
                    end
                elseif obj:IsA("Decal") or obj:IsA("Texture") then
                    local texture = tostring(obj.Texture)
                    if texture == item.oldId or 
                       texture == "rbxassetid://" .. item.oldId or
                       texture:find(item.oldId) then
                        obj.Texture = "rbxassetid://" .. item.newId
                    end
                end
            end
        end
    end

    -- éŸ³æ•ˆæ›¿æ¢
    for _, item in ipairs(replacementLists.Sounds) do
        if item.old ~= "" and item.new ~= "" then
            for _, obj in pairs(game:GetDescendants()) do
                if obj:IsA("Sound") then
                    local soundId = tostring(obj.SoundId)
                    if soundId:find(item.old) then
                        obj.SoundId = "rbxassetid://" .. item.new
                    end
                end
            end
        end
    end

    -- å®æ—¶ç›‘æ§
    game.DescendantAdded:Connect(function(obj)
        task.wait(0.1)

        -- å›¾ç‰‡
        if obj:IsA("ImageLabel") or obj:IsA("ImageButton") then
            local image = tostring(obj.Image)
            for _, item in ipairs(replacementLists.Images) do
                if item.oldId ~= "" and item.newId ~= "" then
                    if image == item.oldId or 
                       image == "rbxassetid://" .. item.oldId or
                       image:find(item.oldId) then
                        obj.Image = "rbxassetid://" .. item.newId
                    end
                end
            end
        end

        -- éŸ³æ•ˆ
        if obj:IsA("Sound") then
            local soundId = tostring(obj.SoundId)
            for _, item in ipairs(replacementLists.Sounds) do
                if item.old ~= "" and item.new ~= "" then
                    if soundId:find(item.old) then
                        obj.SoundId = "rbxassetid://" .. item.new
                    end
                end
            end
        end
    end)
end

