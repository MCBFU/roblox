local progress = 0
local function smoothProgress()
    local startTime = tick()
    while progress < 100 do
        local elapsed = tick() - startTime
        progress = math.clamp((elapsed / 3) * 100, 0, 100)
        TweenService:Create(progressBar,TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{Size = UDim2.new(progress / 100, 0, 1, -2)}):Play()
        progressText.Text = string.format("%d", math.floor(progress))
        RunService.RenderStepped:Wait()
    end
end

task.spawn(smoothProgress)
repeat task.wait(0.1) until progress >= 100
progressText.Text = "100"
task.wait(0.3)

local fadeOutSequence = {
    TweenService:Create(progressContainer,TweenInfo.new(0.5, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out),{BackgroundTransparency = 1}),
    TweenService:Create(progressText,TweenInfo.new(0.5, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out),{TextTransparency = 1}),
    TweenService:Create(mainImage,TweenInfo.new(1.2, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out),{ImageTransparency = 1})
}

for i, tween in ipairs(fadeOutSequence) do
    tween:Play()
    task.wait(0.2)
end

