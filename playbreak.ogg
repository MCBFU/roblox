local function setupDynamicLighting()
    while true do
        task.wait(5)
        
        -- è½»å¾®çš„å…‰å½±åŠ¨ç”»
        local timeVariation = math.sin(os.clock() * 0.2) * 0.5 + 0.5
        
        -- åŠ¨æ€è°ƒæ•´å…‰æ™•å¼ºåº¦
        bloom.Intensity = 0.35 + timeVariation * 0.1
        
        -- åŠ¨æ€è°ƒæ•´å¤ªé˜³å…‰
        sunRays.Intensity = 0.15 + timeVariation * 0.05
        
        -- è½»å¾®çš„é¢œè‰²æ¸©åº¦å˜åŒ–
        local temp = 220 + timeVariation * 35
        colorCorrection.TintColor = Color3.fromRGB(255, 240 - timeVariation * 10, 225 - timeVariation * 20)
    end
end

