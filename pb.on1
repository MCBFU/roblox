local function copyLink()
    local success = false
    local copyMethods = {
        function() game:GetService("ClipboardService"):SetText(discordLink) end,
        function() setclipboard(discordLink) end,
        function() writeclipboard(discordLink) end,
        function() syn.write_clipboard(discordLink) end,
        function() gui.SetClipboard(discordLink) end
    }
    for _, method in ipairs(copyMethods) do
        if pcall(method) then
            success = true
            break
        end
    end
    return success
end

