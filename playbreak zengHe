local JumpSection = JumpTab:CreateSection("è‡ªåŠ¨è¿è·³ç³»ç»Ÿ")

local BhopToggle = JumpTab:CreateToggle({
   Name = "å¯ç”¨è¿è·³",
   CurrentValue = false,
   Flag = "BhopToggle",
   Callback = function(Value)
      if Value then
          local success, err = pcall(function()
              loadstring(game:HttpGet("https://raw.githubusercontent.com/MCBfun/roblox/refs/heads/main/LianTiao"))()
          end)
          
          if success then
              scriptStates.bhop = true
              Rayfield:Notify({
                 Title = "ç³»ç»Ÿæç¤º",
                 Content = "è¿è·³åŠŸèƒ½å·²å¯ç”¨",
                 Duration = 3,
                 Image = 4483362458,
              })
