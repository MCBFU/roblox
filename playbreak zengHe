-- åˆ›å»ºç¥¨æ•°åŠŸèƒ½æ ‡ç­¾é¡µ
local VoteTab = Window:CreateTab("ç¥¨æ•°åŠŸèƒ½", nil)
local VoteSection = VoteTab:CreateSection("è‡ªåŠ¨åˆ·ç¥¨ç³»ç»Ÿ")

local VoteButton = VoteTab:CreateButton({
   Name = "å¯ç”¨åˆ·ç¥¨åŠŸèƒ½",
   Callback = function()
      if not scriptStates.vote then
          local success, err = pcall(function()
              loadstring(game:HttpGet("https://raw.githubusercontent.com/MCBfun/roblox/refs/heads/main/ShuaPiao"))()
          end)
          
          if success then
              scriptStates.vote = true
              Rayfield:Notify({
                 Title = "ç³»ç»Ÿæç¤º",
                 Content = "åˆ·ç¥¨åŠŸèƒ½å·²æˆåŠŸå¯ç”¨",
                 Duration = 3,
                 Image = 4483362458,
              })
              VoteButton:Set("ç¦ç”¨åˆ·ç¥¨åŠŸèƒ½")
          else
              Rayfield:Notify({
                 Title = "é”™è¯¯",
                 Content = "åˆ·ç¥¨åŠŸèƒ½åŠ è½½å¤±è´¥",
                 Duration = 5,
                 Image = 4483362458,
              })
          end
      else
          -- è¿™é‡Œå¯ä»¥æ·»åŠ ç¦ç”¨åˆ·ç¥¨çš„é€»è¾‘
          scriptStates.vote = false
          Rayfield:Notify({
             Title = "ç³»ç»Ÿæç¤º",
             Content = "åˆ·ç¥¨åŠŸèƒ½å·²ç¦ç”¨",
             Duration = 3,
             Image = 4483362458,
          })
          VoteButton:Set("å¯ç”¨åˆ·ç¥¨åŠŸèƒ½")
      end
   end,
})

-- åˆ›å»ºè¿è·³åŠŸèƒ½æ ‡ç­¾é¡µ
