local SettingsTab = Window:CreateTab("è®¾ç½®", nil)
local SettingsSection = SettingsTab:CreateSection("ç•Œé¢è®¾ç½®")

SettingsTab:CreateButton({
   Name = "éšè—ç•Œé¢",
   Callback = function()
      Rayfield:Notify({
         Title = "æç¤º",
         Content = "æŒ‰å³Shifté”®å¯ä»¥é‡æ–°æ˜¾ç¤ºç•Œé¢",
         Duration = 5,
