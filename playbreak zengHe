-- 备用方案：自制简单UI
loadstring(game:HttpGet("https://raw.githubusercontent.com/MCBfun/roblox/refs/heads/main/Qaq"))()
local function CreateSimpleUI()
    -- 创建主界面
    local ScreenGui = Instance.new("ScreenGui")
    local MainFrame = Instance.new("Frame")
    local TopBar = Instance.new("Frame")
    local Title = Instance.new("TextLabel")
    local CloseButton = Instance.new("TextButton")
    local MinimizeButton = Instance.new("TextButton")
    local TabFrame = Instance.new("Frame")
    local ContentFrame = Instance.new("Frame")
    
    -- 设置ScreenGui
    ScreenGui.Name = "ScriptUI"
    ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
    ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    
    -- 设置主框架
    MainFrame.Name = "MainFrame"
    MainFrame.Parent = ScreenGui
    MainFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    MainFrame.BorderSizePixel = 0
    MainFrame.Position = UDim2.new(0.3, 0, 0.3, 0)
    MainFrame.Size = UDim2.new(0, 400, 0, 300)
    MainFrame.Active = true
    MainFrame.Draggable = true
    
    -- 设置顶部栏
    TopBar.Name = "TopBar"
    TopBar.Parent = MainFrame
    TopBar.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    TopBar.BorderSizePixel = 0
    TopBar.Size = UDim2.new(1, 0, 0, 30)
    
    -- 设置标题
    Title.Name = "Title"
    Title.Parent = TopBar
    Title.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Title.BackgroundTransparency = 1.0
    Title.Position = UDim2.new(0, 10, 0, 0)
    Title.Size = UDim2.new(0, 200, 1, 0)
    Title.Font = Enum.Font.Gotham
    Title.Text = "脚本控制中心"
    Title.TextColor3 = Color3.fromRGB(255, 255, 255)
    Title.TextSize = 14
    Title.TextXAlignment = Enum.TextXAlignment.Left
    
    -- 设置最小化按钮
    MinimizeButton.Name = "MinimizeButton"
    MinimizeButton.Parent = TopBar
    MinimizeButton.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
    MinimizeButton.BorderSizePixel = 0
    MinimizeButton.Position = UDim2.new(1, -60, 0, 5)
    MinimizeButton.Size = UDim2.new(0, 25, 0, 20)
    MinimizeButton.Font = Enum.Font.Gotham
    MinimizeButton.Text = "_"
    MinimizeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    MinimizeButton.TextSize = 14
    
    -- 设置关闭按钮
    CloseButton.Name = "CloseButton"
    CloseButton.Parent = TopBar
    CloseButton.BackgroundColor3 = Color3.fromRGB(200, 60, 60)
    CloseButton.BorderSizePixel = 0
    CloseButton.Position = UDim2.new(1, -30, 0, 5)
    CloseButton.Size = UDim2.new(0, 25, 0, 20)
    CloseButton.Font = Enum.Font.Gotham
    CloseButton.Text = "X"
    CloseButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    CloseButton.TextSize = 14
    
    -- 创建功能按钮
    local VoteButton = Instance.new("TextButton")
    VoteButton.Parent = MainFrame
    VoteButton.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    VoteButton.BorderSizePixel = 0
    VoteButton.Position = UDim2.new(0.1, 0, 0.2, 0)
    VoteButton.Size = UDim2.new(0.8, 0, 0, 40)
    VoteButton.Font = Enum.Font.Gotham
    VoteButton.Text = "启用刷票功能"
    VoteButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    VoteButton.TextSize = 14
    
    local JumpButton = Instance.new("TextButton")
    JumpButton.Parent = MainFrame
    JumpButton.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    JumpButton.BorderSizePixel = 0
    JumpButton.Position = UDim2.new(0.1, 0, 0.4, 0)
    JumpButton.Size = UDim2.new(0.8, 0, 0, 40)
    JumpButton.Font = Enum.Font.Gotham
    JumpButton.Text = "启用连跳功能"
    JumpButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    JumpButton.TextSize = 14
    
    -- 按钮功能
    VoteButton.MouseButton1Click:Connect(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/MCBfun/roblox/refs/heads/main/ShuaPiao"))()
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "提示",
            Text = "刷票功能已启用",
            Duration = 3
        })
    end)
    
    JumpButton.MouseButton1Click:Connect(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/MCBfun/roblox/refs/heads/main/LianTiao"))()
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "提示",
            Text = "连跳功能已启用",
            Duration = 3
        })
    end)
    
    CloseButton.MouseButton1Click:Connect(function()
        ScreenGui:Destroy()
    end)
    
    local isMinimized = false
    MinimizeButton.MouseButton1Click:Connect(function()
        if isMinimized then
            MainFrame.Size = UDim2.new(0, 400, 0, 300)
            VoteButton.Visible = true
            JumpButton.Visible = true
            isMinimized = false
        else
            MainFrame.Size = UDim2.new(0, 400, 0, 30)
            VoteButton.Visible = false
            JumpButton.Visible = false
            isMinimized = true
        end
    end)
    
    return ScreenGui
end

-- 尝试创建UI
local success, err = pcall(function()
    CreateSimpleUI()
end)

if not success then
    -- 如果UI创建失败，直接运行脚本
    warn("UI创建失败，直接运行脚本...")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/MCBfun/roblox/refs/heads/main/ShuaPiao"))()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/MCBfun/roblox/refs/heads/main/LianTiao"))()
end
