              Rayfield:Notify({
                 Title = "é”™è¯¯",
                 Content = "è¿è·³åŠŸèƒ½åŠ è½½å¤±è´¥",
                 Duration = 5,
                 Image = 4483362458,
              })
              BhopToggle:Set(false)
          end
      else
          scriptStates.bhop = false
          Rayfield:Notify({
             Title = "ç³»ç»Ÿæç¤º",
             Content = "è¿è·³åŠŸèƒ½å·²å…³é—­",
             Duration = 3,
             Image = 4483362458,
          })
      end
   end,
