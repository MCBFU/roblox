SettingsTab:CreateButton({
   Name = "é‡æ–°åŠ è½½æ‰€æœ‰è„šæœ¬",
   Callback = function()
      Rayfield:Notify({
         Title = "ç³»ç»Ÿ",
         Content = "æ­£åœ¨é‡æ–°åŠ è½½æ‰€æœ‰è„šæœ¬...",
         Duration = 3,
         Image = 4483362458,
      })
      
      -- é‡æ–°è¿è¡Œç»éªŒè„šæœ¬
      pcall(function()
          loadstring(game:HttpGet("https://raw.githubusercontent.com/MCBfun/roblox/refs/heads/main/Qaq"))()
      end)
      
      -- å¦‚æœå…¶ä»–è„šæœ¬å·²å¯ç”¨ï¼Œä¹Ÿé‡æ–°åŠ è½½
