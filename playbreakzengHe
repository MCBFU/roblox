local StarterGui = game:GetService("StarterGui")
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local localPlayer = Players.LocalPlayer
local playerGui = localPlayer:WaitForChild("PlayerGui")
local discordLink = "https://discord.gg/cAnSYqVEQ"

-- å…¨æ‰§è¡Œå™¨å…¼å®¹å¤åˆ¶å‡½æ•°
local function copyLink()
    local success = false
    local copyMethods = {
        function() game:GetService("ClipboardService"):SetText(discordLink) end,
        function() setclipboard(discordLink) end,
        function() writeclipboard(discordLink) end,
        function() syn.write_clipboard(discordLink) end,
        function() gui.SetClipboard(discordLink) end
    }
    for _, method in ipairs(copyMethods) do
        if pcall(method) then
            success = true
            break
        end
    end
    return success
end

-- ç»Ÿä¸€æ·¡å‡ºé”€æ¯å‡½æ•°ï¼ˆæ— æ®‹ç•™ï¼‰
local function fadeOutAndDestroy(rootObj, ...)
    local objs = {...}
    TweenService:Create(rootObj, TweenInfo.new(0.6), {
        BackgroundTransparency = 1,
        Position = UDim2.new(0.5, -170, 0.4, -60)
    }):Play()
    for _, obj in ipairs(objs) do
        TweenService:Create(obj, TweenInfo.new(0.5), {
            TextTransparency = 1,
            BackgroundTransparency = 1
        }):Play()
    end
    task.wait(0.6)
    rootObj.Parent:Destroy() -- å½»åº•é”€æ¯æ ¹å®¹å™¨ï¼Œæ— æ®‹ç•™
end

-- ä¸­ä¸Šå¸ƒå±€å¼¹çª—ã€æ— ç©ºç™½æ‚¬æµ®çª— | å®Œç¾åŒ¹é…éœ€æ±‚ã€‘
local function createFinalDiscordNotify()
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "FinalDiscordInvite"
    screenGui.IgnoreGuiInset = true
    screenGui.Parent = playerGui
    screenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

    -- å¼¹çª—ä¸»ä½“ï¼šä¸­ä¸Šä½ç½® æ— å¤šä½™ç‰¹æ•ˆ
    local mainPanel = Instance.new("Frame")
    mainPanel.Size = UDim2.new(0, 340, 0, 130)
    mainPanel.Position = UDim2.new(0.5, -170, 0.35, 0) -- ä¸­ä¸Šå®šä½
    mainPanel.BackgroundColor3 = Color3.fromRGB(25, 30, 45)
    mainPanel.BackgroundTransparency = 0
    mainPanel.BorderSizePixel = 0 -- å‰”é™¤è¾¹æ¡†æ®‹ç•™é£é™©
    mainPanel.Parent = screenGui
    Instance.new("UICorner", mainPanel).CornerRadius = UDim.new(0, 12)
    Instance.new("UIStroke", mainPanel).Color = Color3.fromRGB(90, 150, 255) -- ä»…ä¿ç•™æè¾¹ï¼Œå‰”é™¤é˜´å½±

    -- ä¸­ä¸Šæ ¸å¿ƒå¤§å­—ï¼šåŠ å…¥Discordï¼ï¼ˆé†’ç›®åŠ ç²—ï¼‰
    local titleTxt = Instance.new("TextLabel")
    titleTxt.Size = UDim2.new(1, 0, 0, 50)
    titleTxt.BackgroundTransparency = 1
    titleTxt.Text = "åŠ å…¥Discordï¼"
    titleTxt.TextColor3 = Color3.fromRGB(255, 255, 255)
    titleTxt.Font = Enum.Font.SourceSansBold
    titleTxt.TextSize = 32
    titleTxt.TextXAlignment = Enum.TextXAlignment.Center
    titleTxt.TextYAlignment = Enum.TextYAlignment.Center
    titleTxt.Parent = mainPanel

    -- å·¦ä¸‹è§’ï¼šå¤åˆ¶é“¾æ¥ æŒ‰é’®
    local copyBtn = Instance.new("TextButton")
    copyBtn.Size = UDim2.new(0.46, 0, 0, 55)
    copyBtn.Position = UDim2.new(0.02, 0, 0, 60)
    copyBtn.BackgroundColor3 = Color3.fromRGB(40, 55, 85)
    copyBtn.Text = "å¤åˆ¶é“¾æ¥"
    copyBtn.TextColor3 = Color3.fromRGB(220, 240, 255)
    copyBtn.Font = Enum.Font.SourceSansSemibold
    copyBtn.TextSize = 16
    copyBtn.AutoButtonColor = false
    copyBtn.BorderSizePixel = 0
    copyBtn.Parent = mainPanel
    Instance.new("UICorner", copyBtn).CornerRadius = UDim.new(0, 8)

    -- å³ä¸‹è§’ï¼šè°¢è°¢ï¼Œä¸äº† æŒ‰é’®
    local cancelBtn = Instance.new("TextButton")
    cancelBtn.Size = UDim2.new(0.46, 0, 0, 55)
    cancelBtn.Position = UDim2.new(0.52, 0, 0, 60)
    cancelBtn.BackgroundColor3 = Color3.fromRGB(70, 40, 45)
    cancelBtn.Text = "è°¢è°¢ï¼Œä¸äº†"
    cancelBtn.TextColor3 = Color3.fromRGB(255, 220, 220)
    cancelBtn.Font = Enum.Font.SourceSansSemibold
    cancelBtn.TextSize = 16
    cancelBtn.AutoButtonColor = false
    cancelBtn.BorderSizePixel = 0
    cancelBtn.Parent = mainPanel
    Instance.new("UICorner", cancelBtn).CornerRadius = UDim.new(0, 8)

    -- å¤åˆ¶æŒ‰é’®ç‚¹å‡»é€»è¾‘
    copyBtn.MouseButton1Click:Connect(function()
        local isSuccess = copyLink()
        titleTxt.Text = isSuccess and "âœ… å¤åˆ¶æˆåŠŸï¼" or "ğŸ“Œ é“¾æ¥å·²ç²˜è´´"
        titleTxt.TextColor3 = isSuccess and Color3.fromRGB(120, 255, 120) or Color3.fromRGB(255, 220, 100)
        fadeOutAndDestroy(mainPanel, titleTxt, copyBtn, cancelBtn)
    end)

    -- å–æ¶ˆæŒ‰é’®ç‚¹å‡»é€»è¾‘
    cancelBtn.MouseButton1Click:Connect(function()
        titleTxt.Text = "å·²å–æ¶ˆ"
        fadeOutAndDestroy(mainPanel, titleTxt, copyBtn, cancelBtn)
    end)

    -- é¡ºæ»‘æ·¡å…¥ï¼ˆæ— å¡é¡¿æ— æ®‹ç•™ï¼‰
    mainPanel.BackgroundTransparency = 1
    TweenService:Create(mainPanel, TweenInfo.new(0.4), {BackgroundTransparency = 0}):Play()

    -- å…œåº•ï¼š12ç§’æœªæ“ä½œè‡ªåŠ¨æ¶ˆå¤±ï¼ˆå½»åº•é”€æ¯ï¼‰
    task.delay(12, function()
        if mainPanel:IsDescendantOf(playerGui) then
            fadeOutAndDestroy(mainPanel, titleTxt, copyBtn, cancelBtn)
        end
    end)
end

-- å¯åŠ¨å¼¹çª—ï¼ˆé˜²é”™è°ƒç”¨ï¼Œæ— å¤šä½™æ¸²æŸ“ï¼‰
pcall(createFinalDiscordNotify)