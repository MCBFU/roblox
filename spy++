-- 悬浮正方体脚本启动器（无表情，可拖动）
local Players = game:GetService("Players")
local player = Players.LocalPlayer

-- 等待玩家
if not player then
    repeat task.wait() until Players.LocalPlayer
    player = Players.LocalPlayer
end

local playerGui = player:WaitForChild("PlayerGui")

print("创建悬浮正方体启动器...")

-- 创建悬浮正方体
local cubeGui = Instance.new("ScreenGui")
cubeGui.Name = "FloatingCubeUI"
cubeGui.ResetOnSpawn = false
cubeGui.DisplayOrder = 999
cubeGui.Parent = playerGui

-- 悬浮正方体（可拖动）
local cubeButton = Instance.new("TextButton")
cubeButton.Name = "CubeButton"
cubeButton.Text = "[]"  -- 简单方块符号
cubeButton.Size = UDim2.new(0, 40, 0, 40)
cubeButton.Position = UDim2.new(0, 20, 0, 100)  -- 屏幕左边中间
cubeButton.BackgroundColor3 = Color3.fromRGB(0, 120, 215)
cubeButton.TextColor3 = Color3.new(1, 1, 1)
cubeButton.Font = Enum.Font.GothamBold
cubeButton.TextSize = 16
cubeButton.BorderSizePixel = 2
cubeButton.BorderColor3 = Color3.new(0.8, 0.8, 0.8)
cubeButton.ZIndex = 100

-- 添加圆角
local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 6)
corner.Parent = cubeButton

cubeButton.Parent = cubeGui

print("创建了悬浮正方体")

-- 创建主界面（初始隐藏）
local mainGui = Instance.new("ScreenGui")
mainGui.Name = "ScriptLauncherUI"
mainGui.ResetOnSpawn = false
mainGui.Enabled = false  -- 初始关闭
mainGui.Parent = playerGui

-- 主窗口
local mainFrame = Instance.new("Frame")
mainFrame.Name = "MainFrame"
mainFrame.Size = UDim2.new(0, 350, 0, 450)
mainFrame.Position = UDim2.new(0.5, -175, 0.5, -225)
mainFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
mainFrame.BorderSizePixel = 2
mainFrame.BorderColor3 = Color3.fromRGB(60, 60, 70)
mainFrame.Parent = mainGui

-- 标题栏
local titleBar = Instance.new("Frame")
titleBar.Size = UDim2.new(1, 0, 0, 40)
titleBar.BackgroundColor3 = Color3.fromRGB(0, 90, 180)
titleBar.Parent = mainFrame

local titleLabel = Instance.new("TextLabel")
titleLabel.Text = "脚本启动器"
titleLabel.Size = UDim2.new(1, -80, 1, 0)
titleLabel.Position = UDim2.new(0, 10, 0, 0)
titleLabel.BackgroundTransparency = 1
titleLabel.TextColor3 = Color3.new(1, 1, 1)
titleLabel.Font = Enum.Font.GothamBold
titleLabel.TextSize = 18
titleLabel.TextXAlignment = Enum.TextXAlignment.Left
titleLabel.Parent = titleBar

-- 关闭主界面按钮
local closeBtn = Instance.new("TextButton")
closeBtn.Text = "X"
closeBtn.Size = UDim2.new(0, 30, 0, 30)
closeBtn.Position = UDim2.new(1, -35, 0.5, -15)
closeBtn.BackgroundColor3 = Color3.fromRGB(180, 50, 50)
closeBtn.TextColor3 = Color3.new(1, 1, 1)
closeBtn.Font = Enum.Font.GothamBold
closeBtn.TextSize = 16
closeBtn.Parent = titleBar

-- 脚本列表
local scripts = {
    {name = "动作抓包", url = "https://raw.githubusercontent.com/MCBFU/roblox/refs/heads/main/spy.p3d", color = Color3.fromRGB(0, 140, 190)},
    {name = "图片抓包", url = "https://raw.githubusercontent.com/MCBFU/roblox/refs/heads/main/spy.png", color = Color3.fromRGB(40, 190, 100)},
    {name = "音频抓包", url = "https://raw.githubusercontent.com/MCBFU/roblox/refs/heads/main/spy.ogg", color = Color3.fromRGB(140, 80, 170)},
    {name = "工具合集", url = "https://raw.githubusercontent.com/chinzhuoxuan3-byte/scripttoolshub/refs/heads/main/README.md", color = Color3.fromRGB(210, 110, 30)},
    {name = "汉化DEX", url = "https://gitee.com/cmbhbh/cmbh/raw/master/Bex.lua", color = Color3.fromRGB(210, 70, 50)}
}

-- 创建滚动区域
local scrollFrame = Instance.new("ScrollingFrame")
scrollFrame.Size = UDim2.new(1, -20, 1, -100)
scrollFrame.Position = UDim2.new(0, 10, 0, 50)
scrollFrame.BackgroundTransparency = 1
scrollFrame.ScrollBarThickness = 6
scrollFrame.ScrollBarImageColor3 = Color3.fromRGB(90, 90, 90)
scrollFrame.Parent = mainFrame

local listLayout = Instance.new("UIListLayout")
listLayout.Padding = UDim.new(0, 10)
listLayout.Parent = scrollFrame

-- 状态标签
local statusLabel = Instance.new("TextLabel")
statusLabel.Text = "点击按钮启动脚本"
statusLabel.Size = UDim2.new(1, -20, 0, 25)
statusLabel.Position = UDim2.new(0, 10, 1, -30)
statusLabel.BackgroundTransparency = 1
statusLabel.TextColor3 = Color3.fromRGB(180, 180, 180)
statusLabel.Font = Enum.Font.Gotham
statusLabel.TextSize = 12
statusLabel.Parent = mainFrame

-- 创建脚本按钮
for i, scriptInfo in ipairs(scripts) do
    local buttonFrame = Instance.new("Frame")
    buttonFrame.Size = UDim2.new(1, 0, 0, 70)
    buttonFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 45)
    buttonFrame.BorderSizePixel = 1
    buttonFrame.BorderColor3 = Color3.fromRGB(55, 55, 65)
    buttonFrame.Parent = scrollFrame
    
    -- 图标
    local iconLabel = Instance.new("TextLabel")
    iconLabel.Text = string.sub(scriptInfo.name, 1, 2)
    iconLabel.Size = UDim2.new(0, 50, 0, 50)
    iconLabel.Position = UDim2.new(0, 10, 0.5, -25)
    iconLabel.BackgroundColor3 = scriptInfo.color
    iconLabel.TextColor3 = Color3.new(1, 1, 1)
    iconLabel.Font = Enum.Font.GothamBold
    iconLabel.TextSize = 14
    iconLabel.Parent = buttonFrame
    
    -- 名称
    local nameLabel = Instance.new("TextLabel")
    nameLabel.Text = scriptInfo.name
    nameLabel.Size = UDim2.new(1, -130, 0, 25)
    nameLabel.Position = UDim2.new(0, 70, 0, 10)
    nameLabel.BackgroundTransparency = 1
    nameLabel.TextColor3 = Color3.new(1, 1, 1)
    nameLabel.Font = Enum.Font.GothamBold
    nameLabel.TextSize = 14
    nameLabel.TextXAlignment = Enum.TextXAlignment.Left
    nameLabel.Parent = buttonFrame
    
    -- 启动按钮
    local launchBtn = Instance.new("TextButton")
    launchBtn.Text = "启动"
    launchBtn.Size = UDim2.new(0, 60, 0, 30)
    launchBtn.Position = UDim2.new(1, -70, 0.5, -15)
    launchBtn.BackgroundColor3 = scriptInfo.color
    launchBtn.TextColor3 = Color3.new(1, 1, 1)
    launchBtn.Font = Enum.Font.Gotham
    launchBtn.TextSize = 12
    launchBtn.Parent = buttonFrame
    
    -- 点击事件
    launchBtn.MouseButton1Click:Connect(function()
        statusLabel.Text = "正在加载: " .. scriptInfo.name
        launchBtn.Text = "加载中"
        launchBtn.BackgroundColor3 = Color3.fromRGB(90, 90, 90)
        
        task.wait(0.5)
        
        local success, err = pcall(function()
            loadstring(game:HttpGet(scriptInfo.url))()
        end)
        
        if success then
            statusLabel.Text = "已加载: " .. scriptInfo.name
            launchBtn.Text = "已加载"
            launchBtn.BackgroundColor3 = Color3.fromRGB(0, 180, 0)
            print("成功加载: " .. scriptInfo.name)
        else
            statusLabel.Text = "加载失败"
            launchBtn.Text = "失败"
            launchBtn.BackgroundColor3 = Color3.fromRGB(180, 0, 0)
            print("加载失败: " .. scriptInfo.name)
            warn("错误: " .. tostring(err))
        end
        
        task.wait(2)
        launchBtn.Text = "启动"
        launchBtn.BackgroundColor3 = scriptInfo.color
        statusLabel.Text = "点击按钮启动脚本"
    end)
    
    -- 悬停效果
    buttonFrame.MouseEnter:Connect(function()
        buttonFrame.BackgroundColor3 = Color3.fromRGB(45, 45, 55)
    end)
    
    buttonFrame.MouseLeave:Connect(function()
        buttonFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 45)
    end)
end

-- 更新滚动区域大小
scrollFrame.CanvasSize = UDim2.new(0, 0, 0, listLayout.AbsoluteContentSize.Y)

print("创建了主界面")

-- 显示/隐藏状态
local isUIVisible = false

-- 正方体拖拽功能
local cubeDragging = false
local cubeDragStart
local cubeStartPos

-- 开始拖拽
cubeButton.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        cubeDragging = true
        cubeDragStart = input.Position
        cubeStartPos = cubeButton.Position
        cubeButton.BackgroundColor3 = Color3.fromRGB(0, 160, 230)
    end
end)

-- 结束拖拽
cubeButton.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        cubeDragging = false
        cubeButton.BackgroundColor3 = Color3.fromRGB(0, 120, 215)
    end
end)

-- 拖拽移动
game:GetService("UserInputService").InputChanged:Connect(function(input)
    if cubeDragging and input.UserInputType == Enum.UserInputType.MouseMovement then
        local delta = input.Position - cubeDragStart
        cubeButton.Position = UDim2.new(
            cubeStartPos.X.Scale, cubeStartPos.X.Offset + delta.X,
            cubeStartPos.Y.Scale, cubeStartPos.Y.Offset + delta.Y
        )
    end
end)

-- 正方体点击事件（切换UI显示）
cubeButton.MouseButton1Click:Connect(function()
    -- 只有不是拖拽时才切换UI
    if not cubeDragging then
        isUIVisible = not isUIVisible
        
        if isUIVisible then
            -- 显示UI
            mainGui.Enabled = true
            cubeButton.Text = "[]"
            cubeButton.BackgroundColor3 = Color3.fromRGB(220, 100, 0)  -- 打开时橙色
            print("打开脚本启动器")
        else
            -- 隐藏UI
            mainGui.Enabled = false
            cubeButton.Text = "[]"
            cubeButton.BackgroundColor3 = Color3.fromRGB(0, 120, 215)  -- 关闭时蓝色
            print("关闭脚本启动器")
        end
    end
end)

-- 关闭按钮事件
closeBtn.MouseButton1Click:Connect(function()
    isUIVisible = false
    mainGui.Enabled = false
    cubeButton.BackgroundColor3 = Color3.fromRGB(0, 120, 215)
    print("通过关闭按钮隐藏界面")
end)

-- 标题栏拖拽功能
local dragging = false
local dragStart
local startPos

titleBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = mainFrame.Position
    end
end)

titleBar.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = false
    end
end)

game:GetService("UserInputService").InputChanged:Connect(function(input)
    if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
        local delta = input.Position - dragStart
        mainFrame.Position = UDim2.new(
            startPos.X.Scale, startPos.X.Offset + delta.X,
            startPos.Y.Scale, startPos.Y.Offset + delta.Y
        )
    end
end)

-- 正方体悬停提示
cubeButton.MouseEnter:Connect(function()
    cubeButton.Text = "点击"
    task.wait(1)
    if not cubeDragging then
        cubeButton.Text = "[]"
    end
end)

cubeButton.MouseLeave:Connect(function()
    if not cubeDragging then
        cubeButton.Text = "[]"
    end
end)

print("悬浮正方体启动器加载完成")
print("使用方法:")
print("  1. 屏幕左边有一个蓝色正方体 []")
print("  2. 拖动正方体可以移动位置")
print("  3. 点击正方体打开/关闭脚本启动器")
print("  4. 点击启动按钮运行脚本")
print("  5. 点击X关闭界面")