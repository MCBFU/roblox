}

-- æ•è·æ‰€æœ‰éŸ³é¢‘
function AudioSniffer:CaptureAll()
    self.CapturedSounds = {}
    self.SoundGroups = {}
    
    local function processObject(obj)
        if obj:IsA("Sound") then
            local soundId = tostring(obj.SoundId)
            if soundId and soundId ~= "" then
                local id = string.match(soundId, "%d+")
                if id then
                    self:AddSound(id, soundId, obj, "Sound")
                end
            end
        elseif obj:IsA("SoundGroup") then
            self.SoundGroups[obj.Name] = obj
        end
    end
    
    for _, obj in pairs(game:GetDescendants()) do
        processObject(obj)
    end
    
    for _, soundGroup in pairs(self.SoundGroups) do
        for _, sound in pairs(soundGroup:GetChildren()) do
            if sound:IsA("Sound") then
                local soundId = tostring(sound.SoundId)
                if soundId and soundId ~= "" then
                    local id = string.match(soundId, "%d+")
                    if id then
                        self:AddSound(id, soundId, sound, "SoundGroup:" .. soundGroup.Name)
                    end
                end
            end
        end
    end
    
    print("ğŸµ éŸ³é¢‘æ•è·å®Œæˆ: " .. #self:GetSoundList() .. " ä¸ªéŸ³é¢‘")
    return self:GetSoundList()
end

function AudioSniffer:AddSound(id, url, obj, sourceType)
    if not self.CapturedSounds[id] then
        self.CapturedSounds[id] = {
            id = id,
            url = url,
            object = obj:GetFullName(),
            type = obj.ClassName,
            sourceType = sourceType,
            timestamp = os.time(),
            volume = obj.Volume,
            pitch = obj.Pitch,
            looping = obj.Looped,
            playing = obj.Playing,
            maxDistance = obj.MaxDistance or 100,
            minDistance = obj.MinDistance or 10,
            isPlaying = false  -- æ’­æ”¾çŠ¶æ€
        }
    end
end

-- æ’­æ”¾éŸ³é¢‘åŠŸèƒ½
function AudioSniffer:PlaySound(id)
    local soundData = self.CapturedSounds[id]
    if not soundData then
        warn("âŒ éŸ³é¢‘ä¸å­˜åœ¨: " .. id)
        return
    end
    
    -- å¦‚æœå·²ç»åœ¨æ’­æ”¾ï¼Œå…ˆåœæ­¢
    if self.PlayingSounds[id] then
        self:StopSound(id)
        return
    end
    
    -- åˆ›å»ºæ’­æ”¾å™¨
    local soundPlayer = Instance.new("Sound")
    soundPlayer.SoundId = soundData.url
    soundPlayer.Volume = soundData.volume
    soundPlayer.Pitch = soundData.pitch
    soundPlayer.Looped = soundData.looping
    soundPlayer.Name = "AudioSniffer_Play_" .. id
    soundPlayer.Parent = SoundService
    
    -- æ’­æ”¾
    soundPlayer:Play()
    
    -- è®°å½•
    self.PlayingSounds[id] = {
        player = soundPlayer,
        startTime = os.time(),
        data = soundData
    }
    
    -- æ›´æ–°çŠ¶æ€
    soundData.isPlaying = true
    
    print("â–¶ï¸ æ’­æ”¾éŸ³é¢‘: " .. id)
    
    -- ç›‘å¬ç»“æŸ
    soundPlayer.Ended:Connect(function()
        self:StopSound(id)
    end)
    
    return soundPlayer
end

-- åœæ­¢æ’­æ”¾
function AudioSniffer:StopSound(id)
    local playingSound = self.PlayingSounds[id]
    if playingSound then
        if playingSound.player and playingSound.player.Parent then
            playingSound.player:Stop()
            playingSound.player:Destroy()
        end
        self.PlayingSounds[id] = nil
        
        if self.CapturedSounds[id] then
            self.CapturedSounds[id].isPlaying = false
        end
        
        print("â¹ï¸ åœæ­¢æ’­æ”¾: " .. id)
    end
end

function AudioSniffer:ExportJSON()
    local exportData = {}
    for id, data in pairs(self.CapturedSounds) do
        table.insert(exportData, {
            id = data.id,
            object = data.object,
            type = data.type,
            sourceType = data.sourceType,
            volume = data.volume,
            pitch = data.pitch,
            looping = data.looping,
            timestamp = data.timestamp
        })
    end
    return HttpService:JSONEncode(exportData)
end

function AudioSniffer:GetSoundList()
    local list = {}
    for id, data in pairs(self.CapturedSounds) do
        table.insert(list, data)
    end
    table.sort(list, function(a, b) return a.timestamp > b.timestamp end)
    return list
end

