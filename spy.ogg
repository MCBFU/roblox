-- æ¸…ç†æ‰€æœ‰çª—å£åŒ…æ‹¬è¿™ä¸ªæŒ‰é’®
local function cleanAll()
    local player = Players.LocalPlayer
    if not player then return 0 end
    
    local playerGui = player:WaitForChild("PlayerGui")
    local removedCount = 0
    
    -- è¦æ¸…ç†çš„çª—å£åå­—
    local targetNames = {
        "AdvancedImageSniffer",
        "ImageSnifferUI", 
        "ImageSniffer",
        "AudioSnifferUI",
        "CompactImageSniffer",
        "ImageCapture",
        "WindowControlPanel",
        "CleanButtonGui"  -- åŠ ä¸Šè¿™ä¸ªæŒ‰é’®è‡ªå·±
    }
    
    -- æ¸…ç†æ‰€æœ‰ç¬¦åˆæ¡ä»¶çš„çª—å£
    for _, gui in pairs(playerGui:GetChildren()) do
        if gui:IsA("ScreenGui") then
            local guiName = gui.Name
            local isTarget = false
            
            -- ç²¾ç¡®åŒ¹é…
            for _, target in ipairs(targetNames) do
                if guiName == target then
                    isTarget = true
                    break
                end
            end
            
            -- æ¨¡ç³ŠåŒ¹é…
            if not isTarget then
                local lowerName = guiName:lower()
                if string.find(lowerName, "sniffer") or 
                   string.find(lowerName, "capture") or
                   string.find(lowerName, "clean") then  -- æ¸…ç†æŒ‰é’®ç›¸å…³
                    isTarget = true
                end
            end
            
            if isTarget then
                gui:Destroy()
                removedCount = removedCount + 1
            end
        end
    end
    
    return removedCount
end

-- å…ˆæ¸…ç†æ—§çš„
